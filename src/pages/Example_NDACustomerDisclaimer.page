<apex:page Controller="contactret" action="{!redirectAutomate}" showHeader="false" sidebar="false">
    <apex:form >
        <apex:pageBlock >
            <apex:pageBlockSection >
                <apex:selectcheckboxes layout="pageDirection">
                    <apex:selectOption itemLabel="I Agree" itemValue="a" />
                </apex:selectcheckboxes>
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
                <apex:commandButton value="Submit" action="{!redirect}" />
            </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form>
</apex:page>